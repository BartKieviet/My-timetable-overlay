{
  "manifest_version": 3,
  "name": "MyTimetable QOL Package",
  "version": "2.0.2",
  "permissions": [
   "storage"
  ],
  "content_scripts": [
  	{
		"matches": ["https://mytimetable.hhs.nl/*"],
		"js": ["timetable-overlay.js"]
	},
	{
		"matches": ["https://hhs.topdesk.net/*"],
		"js": ["topdesk-overlay.js"],
		"all_frames": true,
		"match_about_blank": true,
		"match_origin_as_fallback": true
	}
	]
}